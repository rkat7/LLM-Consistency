# API Keys
OPENAI_API_KEY=your_openai_api_key_here

# LLM Model Settings
LLM_PROVIDER=openai
LLM_MODEL=gpt-4

# Verification Engine Settings
SOLVER_TYPE=z3
MAX_VERIFICATION_ITERATIONS=5
TIMEOUT_SECONDS=30

# Rule Extraction Settings
USE_LLM_FOR_EXTRACTION=False
USE_SPACY_EXTRACTOR=True

# Repair Settings
MAX_REPAIR_ATTEMPTS=3

# Log Settings
LOG_LEVEL=INFO
LOG_FILE=logs/consistency_verifier.log

# Performance Settings
CACHE_RESULTS=True
PARALLEL_PROCESSING=False

# Rate Limiting and API Resilience
API_CALLS_PER_MINUTE=20
MAX_API_RETRIES=5
USE_DISK_CACHE=True
DISK_CACHE_DIR=.cache 